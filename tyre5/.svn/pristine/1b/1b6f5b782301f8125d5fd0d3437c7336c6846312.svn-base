<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="TyreBrand" >
	<typeAlias alias="TyreBrand" type="com.kkbc.entity.TyreBrand"/>
	
	<select id="getTyreBrandList" resultClass="TyreBrand">
		SELECT id,name,pin_yin FROM tyre_brand ORDER BY pin_yin ASC 
	</select>
</sqlMap>