@charset "utf-8";
/* CSS Document */
*{ margin:0 auto; padding:0;}
ul,h1,h2,h3,h4,h5,h6,p,dd,dl,dt,img,li,p{ margin:0; padding:0; border:0;}
ul,li { list-style:none; text-align:left;}
body{font-size:12px;color:#666; font-family:Arial, Helvetica, sans-serif,"����";  line-height:24px; background:url(../images/bg-border.png) repeat-y center #fff; *background:url(../images/login-bg.jpg) no-repeat 50% 100%; }



a {color:#666; text-decoration:none;blr:expression(this.onFocus=this.blur());outline: none; } 
a:hover { color:#026cb6;text-decoration:none;} 
h3{ color:#0530a5; font-size:14px; font-weight:bold;font-family:'΢���ź�','����'; background:url(../images/login-btn.png) no-repeat left; padding-left:10px;}
h3 a{color:#0530a5; }
h3 a:hover{color:#c00000; text-decoration:underline;}
clear{clear:both; margin:0; padding:0; border:0;}
a.more{color:#0530a5; text-align:right; float:right;}
a.more:hover{color:#0530a5; text-decoration:underline;}
a.red-more { color:#c00000;}
a.red-more:hover{ color:#c00000;text-decoration:underline;}
.red{ color:#c00000;}

/*------------------------------------------��¼ҳ��------------------------------------------*/
.box{ width:100%; height:100%; margin:0 auto;background:url(../images/login-bg.jpg) no-repeat 50% 100%; display:block; position:fixed; }
.login-mid{ width:1024px; margin:0 auto;}
.login-box{ width:290px; height:196px; left:52%; bottom:220px; position:absolute;}
.login-box strong{/*width:70%;  float:left; */color:#f00; padding-left:60px;line-height:30px;}
.login-box dl{ width:100%; float:left;}
.login-box dd{ width:286px; float:left; line-height:48px;}
.login-box dd p{ width:286px; float:left; margin-top:1px;}
.login-box dd img{ width:76px; height:28px;  float:left;  border:1px #cae0f8 solid;}
.login-box dd span{ width:80px; text-align:right; float:left;}
.login-box dd input{ width:204px; height:24px; padding:2px 5px; border:1px #cae0f8 solid; float:left; margin-top:6px;}
.login-box dd input:hover{ border:1px #0065ff solid;}
.login-box dt{ width:100%; float:left; margin-top:5px;}
.login-btn{ width:76px; height:27px; background:url(../images/login-btn.png) no-repeat; display:block; border:none; float:left; margin-left:80px; display:inline;}
.submit-btn{ width:76px; height:27px; background:url(../images/submit-btn.png) no-repeat; display:block; border:none; float:left; margin-left:80px; display:inline;}
.back-btn{ width:76px; height:27px; background:url(../images/back-btn.png) no-repeat; display:inline; border:none; float:left; margin-left:20px; display:inline;}
.login-box dt div{ width:100%; float:left; text-align:center; line-height:48px;}
.login-box dt div a{ margin:0 5px;}
#land-in{ width:125px; float:left;margin-right:7px;}
.login-for{ text-decoration:underline; padding-left:10px; line-height:32px;}
.login-foot{ width:1024px; height:24px; text-align:center; padding:10px 0;  bottom:0; position:absolute;}
#warp_cent{ overflow:hidden; z-index:1;}
#floatdiv{ overflow:hidden; z-index:2;}
