<script type="text/javascript">
var istrueans = false; 

$(function(){
 $(".animals ul li").click(function(){
   if($(this).find('img.check').length==0){
		$(this).find(".animals_box").append('<img class="check" src="images/five/1/check.png" />');
	  }else{
		$(this).find('img.check').remove();
   }
	 });
 }); 
 
 function tijiao(){
	 var trueans = [3,11,20,23,30,32];
	 var ans = [];
	 var srore = 0 ;
	 $.each($(".animals ul li img.check"),function(){
		 var imgsrc = $(this).prev().attr("src");
		 var srrStr = imgsrc.substring(imgsrc.lastIndexOf("/")+1, imgsrc.lastIndexOf("."));
		 ans.push(srrStr);
	 });
	 
	 if(trueans.sort().toString()==ans.sort().toString() && istrueans==true){
		 srore = 1;
	 }
	 
	 var ansdata = {
 			dimension1:"第五大题",
 			dimension2:"第1小题",
 			que_no:"1",
 			true_answer: trueans.sort().toString(),
 			answers: ans.sort().toString(),
 			score:srore
     	};
 
	 $.ajax({
			type: "POST",
			url: "addAnswerOneanswer.action",
			data:ansdata,
			async: false,
			success: function(msg){	
				alert("恭喜你完成测试！");
				$("#suba").removeAttr('href');
				$("#suba").removeAttr('onclick');
				 window.setTimeout("goto()",10000);
			}			
		});
	 
 }
 
 function istrue(){
	 if($(".check").length==0){
		 istrueans = true;
	 }else{
		 istrueans = false;
	 }
 }
 
 function goto(){
	 window.location.href = "/questions/";
 }

</script>
 
