package com.kkbc.entity;

import java.util.Date;

public class UserHeart {
	
	private int heart_id;
	private String openId;
	private int desc_id;
	private Date create_time;
	
	
	
	public UserHeart(){}
	public UserHeart(int desc_id,String openId,Date date){
		this.desc_id=desc_id;
		this.openId=openId;
		this.create_time=date;
	}
	public int getHeart_id() {
		return heart_id;
	}
	public void setHeart_id(int heart_id) {
		this.heart_id = heart_id;
	}
	public String getOpenId() {
		return openId;
	}
	public void setOpenId(String openId) {
		this.openId = openId;
	}
	public int getDesc_id() {
		return desc_id;
	}
	public void setDesc_id(int desc_id) {
		this.desc_id = desc_id;
	}
	public Date getCreate_time() {
		return create_time;
	}
	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}
	

}
